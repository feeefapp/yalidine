{
  "name": "yalidine",
  "description": "non-official, type-safe JavaScript/TypeScript SDK for Yalidine and Guepex delivery APIs, by feeef.org team",
  "version": "0.1.0",
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build",
    "!build/tests"
  ],
  "exports": {
    ".": "./build/index.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "quick:test": "node --loader=ts-node/esm --enable-source-maps bin/test.ts",
    "clean": "del-cli build",
    "typecheck": "tsc --noEmit",
    "precompile": "npm run lint && npm run clean",
    "compile": "tsup-node && tsc --emitDeclarationOnly --declaration",
    "build": "npm run compile",
    "release": "npx release-it",
    "version": "npm run build",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@japa/assert": "^3.0.0",
    "@japa/expect-type": "^2.0.2",
    "@japa/runner": "^3.1.4",
    "@japa/snapshot": "^2.0.5",
    "@swc/core": "^1.5.3",
    "@types/dlv": "^1.1.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.10",
    "ajv": "^8.13.0",
    "benchmark": "^2.1.4",
    "c8": "^9.1.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "github-label-sync": "^2.3.1",
    "husky": "^9.0.11",
    "joi": "^17.13.1",
    "prettier": "^3.2.5",
    "release-it": "^17.2.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "axios-cache-interceptor": "^1.5.1",
    "luxon": "^3.5.0",
    "vitest": "^3.2.4"
  },
  "author": "mohamadlounnas,feeefnet",
  "license": "MIT",
  "homepage": "https://github.com/feeefapp/yalidine#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feeefapp/yalidine.git"
  },
  "bugs": {
    "url": "https://github.com/feeefapp/yalidine/issues"
  },
  "keywords": [],
  "types": "./build/index.d.ts",
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "directories": {
    "test": "tests"
  },
  "tsup": {
    "entry": [
      "./index.ts"
    ],
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": true,
    "target": "esnext"
  }
}
